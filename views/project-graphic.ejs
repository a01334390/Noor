	<div class="container-fluid portfolio-single-content alia ">
		<div class="row">
			<div class="banner-wrap">
				<h3 class="title"><%= project[0].name %></h3>
				<img src="<%= project[0].mainimg %>" class="s-img-switch" alt="" />    
			</div>
			<div class="container">
				<div class="row">
					<div class="text-gallery-wrap">
						<div class="additional-text"><%= project[0].quote %></div>
						<div class="info-item-wrap">
							<div class="item">
								<div class="name">Date</div>
								<div class="text-item"><%= project[0].date %></div>
							</div>
							<div class="item">
								<div class="name">Clients</div>
								<% for(var i = 0; i < project[0].clients.length; i++){ %>
								<div class="text-item">
									<%= project[0].clients[i] %>
								</div>
								<% } %>
							</div>
							<div class="item">
								<div class="name">Tags</div>
								<div class="text-item">
										<% for(var i = 0; i < project[0].category.length; i++) {%>
											<% if(i+1 == project[0].category[i].length){ %>
													<%= project[0].category[i] %>,
											<% } else {%>
												 <%= project[0].category[i] %>
											<% } %>
										<% } %>
								</div>
							</div>
							<div class="item">
								<div class="name">Category</div>
								<div class="text-item">
									<%= project[0].overall %>
								</div>
							</div>
						</div>
						<div class="btn-wrapper">
							<a href="<%= project[0].video %>" class="a-btn">see video</a>
						</div>
					</div>
				</div>
			</div>
			<div class="gallery-wrap-additional izotope-container">
				<% for(var i = 0; i < project[0].gallery.length; i++){ %>
				<img src="<%= project[0].gallery[i] %>" class="gallery-item" alt="" />
				<% }%>
				
			</div>
			<div class="container">
				<div class="row">
					<div class="text-wrap">
						<div class="title-for-text"><%= project[0].title %></div>
						<div class="text">
							<p><%= project[0].p1 %></p>
							<p><%= project[0].p2 %></p>
						</div>
					</div>
				</div>
			</div>
			<div class="container">
				<div class="row">
					<div class="gallery-wrap-main light-gallery izotope-container insta-wrapper">
						<% for(var i = 0; i < project[0].shots.length; i++){%>
						<a href="<%=project[0].shots[i]%>" class="gallery-item insta-item">
							<img src="<%=project[0].shots[i]%>" class="" alt="" />
						</a>
						<% } %>
						
					</div>
				</div>
			</div>
			<div class="single-share">
				<div class="ft-part">
					<ul class="social-list">
						<li>
							<a href="<%= project[0].video%>" class="twitter" target="_blank" title="youtube"><i class="fa fa-youtube"></i></a>
						</li>

						<li>
							<a href="<%= project[0].drive%>" class="facebook" target="_blank"><i class="fa fa-google"></i></a>
						</li>
						
					</ul>
				</div>
			</div>
		</div>
		<div class="container">
				<div class="row">
					<div class="single-pagination tile_info">
						<div class="pag-prev">
							<span>
								<span>home</span>
								<a href="/" class="content">Back to home</a>
							</span>
						</div>
						<div class="icon-wrap"><i class="fa fa-th-large"></i></div>
						<div class="pag-next">
							<span>
								<span>Other projects</span>
								<a href="/portfolio" class="content">See more</a>
							</span>
						</div>
					</div>
				</div>
			</div>
	</div>
